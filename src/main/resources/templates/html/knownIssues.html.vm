<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="$messages.locale.language" lang="$messages.locale.language">
<head>
	<script type="text/javascript" src="js/jquery-1.12.0.min.js"></script>
	<script type="text/javascript" src="js/bootstrap.min.js"></script>
	<script type="text/javascript" src="js/canvasjs.min.js"></script>
	<link rel="stylesheet" href="css/bootstrap.min.css" />
	<link rel="stylesheet" href="css/reportng.css" />
</head>
<body>
<h3 class="KnownDefectTitle">$messages.getString("knownIssues")</h3>
<div>
	#if ($utils.issues($suites).getKnownIssuesRegression().size()>0)
	<div class="well well-large">
	<h4>$messages.getString("regressionissues")</h4>
		<table class="table table-bordered">
			<thead>
				<tr>
					<th scope="col" width="25%">$messages.getString("issues")</th>
					<th scope="col" width="25%">$messages.getString("suiteName")</th>
					<th scope="col" width="25%">$messages.getString("testName")</th>
				  	<th scope="col" width="25%">$messages.getString("testClass")</th>
				</tr>
			</thead>
			<tbody>
				$utils.getIssues($utils.issues($suites).getKnownIssuesRegression())
			</tbody>
		</table>
	</div>
	#end
	
	#if ($utils.issues($suites).getKnownIssuesNewFeature().size()>0)
	<div class="well well-large">
	<h4>$messages.getString("newissuesonFeatures")</h4>
		<table class="table table-bordered">
			<thead>
				<tr>
					<th scope="col" width="25%">$messages.getString("issues")</th>
					<th scope="col" width="25%">$messages.getString("suiteName")</th>
					<th scope="col" width="25%">$messages.getString("testName")</th>
				  	<th scope="col" width="25%">$messages.getString("testClass")</th>
				</tr>
			</thead>
			<tbody>
				$utils.getIssues($utils.issues($suites).getKnownIssuesNewFeature())
			</tbody>
		</table>
	</div>
	#end
</div>	
</body>
</html>