<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="$messages.locale.language" lang="$messages.locale.language">
<head>
	<script type="text/javascript" src="js/jquery-1.12.0.min.js"></script>
	<script type="text/javascript" src="js/bootstrap.min.js"></script>
	<script type="text/javascript" src="js/canvasjs.min.js"></script>
	<link href="css/bootstrap.min.css" rel="stylesheet" />
	<link href="css/reportng.css" rel="stylesheet" />
	<script type="text/javascript">  
	    $(document).ready(function () {  
	            $('tr.parent')  
	                .css("cursor", "pointer")  
	                .attr("title", "Click to expand/collapse")  
	                .click(function () {  
	                    $(this).siblings('.child-' + this.id).toggle();  
	                });  
	                $('tr.parent')  
	                .css("cursor", "pointer")  
	                .attr("title", "Click to expand/collapse")  
	                .click();
	    });
	    function changeIcon(elementId) {
			if ($('#'+elementId).hasClass('glyphicon glyphicon-minus')){
				$('#'+elementId).removeClass('glyphicon glyphicon-minus').toggleClass('glyphicon glyphicon-plus');
			}else{
				$('#'+elementId).removeClass('glyphicon glyphicon-plus').toggleClass('glyphicon glyphicon-minus');
			}
		}
    </script>
</head>
<body>
<h3 class="normal">$messages.getString("regression")</h3>	
<div>	
	<div class="well well-large">
		<table class="table table-bordered" width="100%;" id="features">
			<thead>
				<tr>
					<th scope="col" width="30px">#</th>
					<th scope="col" colspan="2">$messages.getString("featureDescription")</th>
					<th scope="col" colspan="1">$messages.getString("totalTests")</th>
				  	<th scope="col" colspan="2">$messages.getString("overallstatus")</th>
				</tr>
			</thead>
			<tbody>
				$utils.getFeatures($utils.issues($suites).getFeature())
			</tbody>
		</table>
	</div>
</div>
</body>
</html>